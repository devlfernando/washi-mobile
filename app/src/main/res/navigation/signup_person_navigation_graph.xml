            app:destination="@id/personDocumentBottomSheet"
            app:launchSingleTop="true"
            app:popUpTo="@id/signup_person_navigation_graph"
            app:popUpToInclusive="true">
            <argument
                android:name="person"
                app:argType="br.com.washi.signup.data.PersonRequest" />
        </action>
    </dialog>
    <dialog
