<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/signup_person_navigation_graph"
    app:startDestination="@id/personNameBottomSheet">

    <dialog
        android:id="@+id/personNameBottomSheet"
        android:name="br.com.washi.signup.view.PersonNameBottomSheet"
        android:label="PersonNameBottomSheet"
        tools:layout="@layout/fragment_person_name_bottom_sheet">
        <action
            android:id="@+id/action_personNameBottomSheet_to_personEmailBottomSheet"
            app:destination="@id/personEmailBottomSheet"
            app:launchSingleTop="true"
            app:popUpTo="@id/signup_person_navigation_graph"
            app:popUpToInclusive="true">
            <argument
                android:name="person"
                app:argType="br.com.washi.signup.data.PersonRequest" />
        </action>
    </dialog>
    <dialog
        android:id="@+id/personEmailBottomSheet"
        android:name="br.com.washi.signup.view.PersonEmailBottomSheet"
